programa
{
	funcao inicio()
	{
		inteiro base, altura, area
		escreva("\n Calcular área do Retângulo \n")
		escreva(":: Informe o valor da base: ")
		leia (base)
		escreva(":: Informe a altura: ")
		leia (altura)
		area = base * altura
		escreva("\n A área deste Retângulo é ",area," m2.")
	}
}
